<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ContenuResa" table="contenu_resa">
    <indexes>
      <index name="reservation" columns="reservation"/>
      <index name="meuble" columns="meuble"/>
    </indexes>
    <id name="reservation" association-key="true"/>
    <one-to-one field="reservation" target-entity="Reservation">
      <join-columns>
        <join-column name="reservation" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="meuble" target-entity="Meuble">
      <join-columns>
        <join-column name="meuble" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
