<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <unique-constraints>
      <unique-constraint name="UNIQ_8D93D64992FC23A" columns="username_canonical"/>
      <unique-constraint name="UNIQ_8D93D649A0D96FB" columns="email_canonical"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adresse" type="text" column="adresse" length="16777215" nullable="false"/>
    <field name="cp" type="text" column="CP" length="16777215" nullable="false"/>
    <field name="ville" type="text" column="ville" length="16777215" nullable="false"/>
    <field name="nom" type="text" column="nom" length="16777215" nullable="false"/>
    <field name="prenom" type="text" column="prenom" length="16777215" nullable="false"/>
    <field name="dateNaissance" type="date" column="date_naissance" nullable="false"/>
    <field name="photo" type="text" column="photo" length="16777215" nullable="true"/>
    <field name="cagnotte" type="integer" column="cagnotte" nullable="true"/>
    <field name="telephone" type="text" column="telephone" length="16777215" nullable="true"/>   
  </entity>
</doctrine-mapping>
